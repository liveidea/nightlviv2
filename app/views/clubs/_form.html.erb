<%= form_for @club do |f| %>
 
  <% if @club.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@club.errors.count, "error") %> prohibited
        this club from being saved:
      </h2>
      <ul>
        <% @club.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
 
  <p>
    <%= f.label :Назва %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :Адреса %><br>
    <%= f.text_field :address %>
  </p>

  <p>
    <%= f.label :Про_клуб %><br>
    <%= f.text_area :about %>
  </p>

  <p>
    <%= f.label :photo %><br>
    <%= f.text_field :photo_id %>
  </p>

  <p>
    <%= f.label :forsquare %><br>
    <%= f.text_field :forsquare_id %>
  </p>


 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
